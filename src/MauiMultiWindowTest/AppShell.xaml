<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiMultiWindowTest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiMultiWindowTest"
    xmlns:pages="clr-namespace:MauiMultiWindowTest.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiMultiWindowTest">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="InShellPage"
        ContentTemplate="{DataTemplate pages:InShellPage}"
        Route="InShellPage"
        />
    
</Shell>
